{
  "name": "scikit-learn-ts",
  "version": "0.0.1",
  "private": true,
  "description": "TODO",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/scikit-learn-ts",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "prebuild": "del build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pretest": "run-s build",
    "test": "run-p test:*",
    "test:unit": "ava",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "got": "^12.6.0",
    "p-map": "^5.5.0",
    "python-bridge": "^1.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/d3-array": "^3.0.4",
    "@types/d3-scale": "^4.0.3",
    "@types/node": "^18.14.4",
    "ava": "^5.2.0",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "tsx": "^3.12.2",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  },
  "keywords": [
    "scikit",
    "learn",
    "machine learning",
    "ml",
    "ai"
  ]
}
